{
  "name": "babel-plugin-unassert",
  "description": "Babel plugin for unassert: Encourages programming with assertions by providing tools to compile them away",
  "version": "3.0.1",
  "author": {
    "name": "Takuto Wada",
    "email": "takuto.wada@gmail.com",
    "url": "https://github.com/twada"
  },
  "bugs": "https://github.com/unassert-js/babel-plugin-unassert/issues",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "mocha": "^8.0.0",
    "semistandard": "^16.0.0",
    "snazzy": "^9.0.0"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "index.js",
    "package.json"
  ],
  "homepage": "https://github.com/unassert-js/babel-plugin-unassert",
  "keywords": [
    "DbC",
    "assert",
    "babel",
    "babel-plugin",
    "unassert"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/unassert-js/babel-plugin-unassert.git"
  },
  "scripts": {
    "lint": "semistandard --verbose index.js test | snazzy",
    "fmt": "semistandard --fix index.js test",
    "test": "mocha --timeout 0"
  },
  "semistandard": {
    "ignore": [],
    "globals": [
      "describe",
      "beforeEach",
      "it"
    ]
